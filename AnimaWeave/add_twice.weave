-- import

add.nodeflow

--

-- graph

nodes {
    add1 add.Add
    add2 add.Add  
}

datas {
    // 外部边界端口会自动推算：
    // 输入边界：add1.a, add1.b (因为没有来源)
    // 输出边界：add2.result (因为没有去向)
    add1.result -> add2.a
    add1.result -> add2.b
}

controls {
    // 控制边界端口自动推算：
    // 输入边界：add1.execute (没有来源)  
    // 输出边界：add2.done (没有去向)
    add1.done -> add2.execute
}

--- 