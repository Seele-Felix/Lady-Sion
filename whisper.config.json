{
  "baseUrl": "http://localhost:8000",
  "timeout": 30000,
  "whisperPath": "/api/whisper",
  "debug": true,
  "enableMetrics": true,
  "retries": 3,
  "retryBackoff": "exponential",
  "auth": {
    "type": "bearer",
    "token": "dev-token-placeholder"
  },
  "headers": {
    "X-API-Version": "v1",
    "X-Client": "lady-sion-web",
    "X-Environment": "development"
  },
  "environment": "development"
} 