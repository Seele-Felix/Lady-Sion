{
  "default": {
    "baseUrl": "http://localhost:8000",
    "timeout": 30000,
    "whisperPath": "/whisper",
    "contentType": "application/json",
    "responseType": "json",
    "retries": 3,
    "retryDelay": 1000,
    "retryBackoff": "exponential",
    "debug": false,
    "enableMetrics": false,
    "validateSSL": true,
    "corsEnabled": true,
    "headers": {
      "Content-Type": "application/json",
      "User-Agent": "WhisperFramework/1.0"
    }
  }
}
